class Base {
    public void area(int a, int b) {
        System.out.println("Area of Square: " + (a * a));
    }
}
class Derived extends Base {
    public void area(int a, int b) {
        System.out.println("Area of Rectangle: " + (a * b));
    }
}
public class Main {
    public static void main(String[] args) {
        Base base = new Base();
        base.area(5, 5);
        Derived derived = new Derived();
        derived.area(4, 6); 
    }
}
